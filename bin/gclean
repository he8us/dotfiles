#!/bin/bash

source $ZSH/bin/helpers/colors.sh

info 'Delete all git branches but master an develop.'
question "If your some branch is not pushed you WILL LOST data. Continue ? [yes/No]" eraseBranches "no"


if [ "${eraseBranches}" == 'yes' ] || [ "${eraseBranches}" == 'y' ]; then
  git branch | grep -v '\*\|master\|develop' | xargs -n 1 git branch -D
fi
